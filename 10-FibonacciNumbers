using System;

class FibonacciNumbers
{
    static void Main(string[] args)
    {
        int n = int.Parse(Console.ReadLine());
        int a = 0;
        int b = 1;
        int temp;
        Console.Write(a);
        for (int i = 0; i < n - 1; i++)
        {
            temp = a;
            a = b;
            b = temp + a;
            Console.Write(" " + a);
        }
        Console.WriteLine();
    }
}
